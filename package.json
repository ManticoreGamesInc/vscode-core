{
  "name": "vscode-core",
  "displayName": "Core Lua API Autocomplete",
  "description": "Provides autocomplete support for the Core Games Lua API",
  "license": "MIT",
  "version": "0.10.0",
  "icon": "images/logo.png",
  "publisher": "ManticoreGames",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManticoreGamesInc/vscode-core"
  },
  "bugs": {
    "url": "https://github.com/ManticoreGamesInc/vscode-core/issues"
  },
  "galleryBanner": {
    "color": "#1A1E26",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./dist/extension",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run test-compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "chai": "^4.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.21",
    "@types/vscode": "1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@typescript-eslint/typescript-estree": "^4.14.0",
    "eslint": "^7.18.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.4.1",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.3.1"
  }
}
