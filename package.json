{
  "name": "vscode-core",
  "displayName": "Core Lua API Autocomplete",
  "description": "Provides autocomplete support for the Core Games Lua API",
  "license": "MIT",
  "version": "0.2.2",
  "icon": "images/logo.png",
  "publisher": "ManticoreGames",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManticoreGamesInc/vscode-core"
  },
  "bugs": {
    "url": "https://github.com/ManticoreGamesInc/vscode-core/issues"
  },
  "galleryBanner": {
    "color": "#1A1E26",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./dist/extension",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run test-compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "chai": "^4.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@typescript-eslint/typescript-estree": "^4.3.0",
    "eslint": "^7.10.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
