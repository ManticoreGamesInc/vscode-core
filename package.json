{
  "name": "vscode-core",
  "displayName": "Core API Autocomplete",
  "description": "Provides autocomplete support for the Core Games API",
  "license": "MIT",
  "version": "0.1.0",
  "icon": "images/logo.png",
  "publisher": "ManticoreGames",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManticoreGamesInc/vscode-core"
  },
  "bugs": {
    "url": "https://github.com/ManticoreGamesInc/vscode-core/issues"
  },
  "galleryBanner": {
    "color": "#E67E17",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@types/request-promise-native": "^1.0.17",
    "chai": "^4.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/vscode": "^1.45.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.9.3",
    "vscode-test": "^1.3.0"
  }
}