{
  "name": "vscode-core",
  "displayName": "Core API Autocomplete",
  "description": "Provides autocomplete support for the Core Games API",
  "license": "MIT",
  "version": "0.1.5",
  "icon": "images/logo.png",
  "publisher": "ManticoreGames",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManticoreGamesInc/vscode-core"
  },
  "bugs": {
    "url": "https://github.com/ManticoreGamesInc/vscode-core/issues"
  },
  "galleryBanner": {
    "color": "#E67E17",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.45.1"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./dist/extension",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run test-compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@types/request-promise-native": "^1.0.17",
    "chai": "^4.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.45.1",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "@typescript-eslint/typescript-estree": "^3.0.2",
    "eslint": "^7.1.0",
    "glob": "^7.1.6",
    "mocha": "^7.2.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "vscode-test": "^1.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
